<p id="notice"><%= notice %></p>

<h1>Denounce Types</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @denounce_types.each do |denounce_type| %>
      <tr>
        <td><%= denounce_type.id %></td>
        <td><%= denounce_type.title %></td>
        <td><%= link_to 'Show', denounce_type %></td>
        <td><%= link_to 'Edit', edit_denounce_type_path(denounce_type) %></td>
        <td><%= link_to 'Destroy', denounce_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Denounce Type', new_denounce_type_path %>
